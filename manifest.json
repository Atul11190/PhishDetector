{
    "manifest_version": 3,
    "name": "PhishDetect",
    "version": "1.0",
    "description": "Detect phishing attempts and alert users.",
    "action": {
    "default_popup": "pop-up/popup.html",
    "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
},
"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
},
"permissions": [
    "activeTab",
    "storage",
    "scripting"
],
"host_permissions": [
    "https://*/*",
    "http://*/*"
]
}
